#include<iostream>
using namespace std;
int main(){
    int marks;
    string grade;
    cout<<"Enter your Marks";
    cin>>marks;
    if(marks>=80 && marks<=100){
        cout<<'A';
    }else if(marks>=70  &&marks<=79){
       cout<<"-A";
    }else if(marks>=60  && marks<=69){
       cout<<"+B";
    }else if(marks>=50  &&  marks<=59){
       cout<<"-B";
    }else if(marks>=40  &&  marks<=49){
       cout<<"c";
    }else{
       cout<<"Fail";
    }
    return 0;
}
