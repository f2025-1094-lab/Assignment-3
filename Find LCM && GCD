#include <iostream>
using namespace std;
int main()
{
    int number1, number2;
    cout << "Enter two numbers ";
    cin >> number1 >> number2;
    int gcd = 1;
    for (int i = 1; i <= number1 && i <= number2; i++) {
        if (number1 % i == 0 && number2 % i == 0) {
            gcd = i;
        }
    }
    cout << "GCD is: " << gcd << endl;
    int lcm;
    int maxNum = (number1 > number2) ? number1 : number2;
    for (int i = maxNum; ; i++) {
        if (i % number1 == 0 && i % number2 == 0) {
            lcm = i;
            break;
        }
    }
    cout << "LCM is: " << lcm << endl;
    return 0;
}
