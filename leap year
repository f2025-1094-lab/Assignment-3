#include<iostream>
using namespace std;
int main(){
    int year;
    cout<<"Enter you Year ";
    cin>>year;
    if(year%100==0){
        if(year%400==0){
            cout<<"it is century year"<<year;
        }else{
            cout<<"it is Normal year"<<year;
        }
    }else{
        if(year%4==0){
            cout<<"it is leap Year "<<year;
        }else{
            cout<<"it is normal Year"<<year;
        }
    }
    return 0;
}
